{
  "name": "mustr",
  "version": "1.0.27",
  "description": "Barebones generator.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "node ./build clean",
    "copy": "node ./build copy",
    "compile": "node ./build compile",
    "bump": "node ./build bump",
    "build": "node ./build build",
    "commit": "node ./build commit",
    "docs": "node ./build docs",
    "release": "node ./build release",
    "test": "node ./build test"
  },
  "bin": {
    "mu": "./bin/mustr"
  },
  "build": {
    "clean": [
      "./dist"
    ],
    "copy": {
      "blueprints": {
        "src": "./src/blueprints",
        "dest": "./dist/blueprints"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/origin1tech/mustr.git"
  },
  "keywords": [
    "Yoeman",
    "Slush",
    "Mustr",
    "Generator"
  ],
  "author": "Origin1 Technologies <origin1tech@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/origin1tech/mustr/issues"
  },
  "homepage": "https://github.com/origin1tech/mustr#readme",
  "dependencies": {
    "async": "^2.3.0",
    "colurs": "^1.0.12",
    "detect-indent": "^5.0.0",
    "front-matter": "^2.1.2",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.1",
    "lodash": "^4.17.4",
    "mustache": "^2.3.0",
    "pargv": "^2.0.6",
    "timbr": "^1.0.1"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/chai": "^3.5.0",
    "@types/fs-extra": "^2.1.0",
    "@types/lodash": "^4.14.61",
    "@types/mocha": "^2.2.40",
    "@types/mustache": "^0.8.29",
    "@types/node": "^7.0.12",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "mocha": "^3.2.0",
    "stiks": "^1.0.14",
    "ts-node": "^3.0.2",
    "typedoc": "^0.5.10",
    "typescript": "^2.2.2"
  }
}
